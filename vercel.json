{
  "buildCommand": "npm run build",
  "routes": [
    {
      "src": "^/(_next|static)/",
      "headers": {
        "Cache-Control": "s-maxage=31536000,immutable"
      },
      "continue": true
    },
    {
      "src": "/[^.]+",
      "dest": "/index.html"
    }
  ],
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node",
      "config": {
        "build": {
          "env": {
            "NPM_FLAGS": "--platform=linux --arch=x64"
          }
        }
      }
    }
  ]
}
